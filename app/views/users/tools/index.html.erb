<h1>Listing teams</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%=h team.name %></td>
    <td><%= link_to 'Show', user_team_path(team) %></td>
    <td><%= link_to 'Edit', edit_user_team_path(team) %></td>
    <td><%= link_to 'Destroy', user_team_path(team), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr><br>
<% end %>
</table>

<br >

<%= link_to 'New team', new_user_team_path %>