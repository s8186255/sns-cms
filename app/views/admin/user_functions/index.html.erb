<h1>Listing user_functions</h1>

<table>
  <tr>
    <th>User</th>
    <th>Function</th>
    <th>Status</th>
    <th>Up to</th>
  </tr>

<% @user_functions.each do |user_function| %>
  <tr>
    <td><%=h user_function.user_id %></td>
    <td><%=h user_function.function_id %></td>
    <td><%=h user_function.status %></td>
    <td><%=h user_function.up_to %></td>
    <td><%= link_to 'Show', admin_user_function_path(user_function) %></td>
    <td><%= link_to 'Edit', edit_admin_user_function_path(user_function) %></td>
    <td><%= link_to 'Destroy', admin_user_function_path(user_function), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_function', new_admin_user_function_path %>